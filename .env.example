VITE_SUPABASE_URL=

VITE_SUPABASE_ANON_KEY=


VITE_APP_NAME=FleetManager
VITE_APP_VERSION=2.0.0
VITE_APP_ENVIRONMENT=development


# Mapbox Token (obtener de: https://account.mapbox.com/)
VITE_MAPBOX_TOKEN=

# Google Maps API Key (obtener de: https://console.cloud.google.com/)
VITE_GOOGLE_MAPS_API_KEY=
VITE_MAPBOX_STYLE_ID=mapbox/satellite-streets-v12



# =====================================================
# Configuración de desarrollo
# =====================================================
# Modo debug (habilita logs adicionales)
VITE_DEBUG_MODE=true

# Modo mock (usa datos simulados en lugar de Supabase)
# Cambia a false cuando tengas Supabase configurado
VITE_MOCK_MODE=false


# =====================================================
# Chatbot (n8n + Ollama)
# =====================================================
# URL del webhook de n8n que recibirá los mensajes del chatbot
# Ejemplo local: http://localhost:5678/webhook/chatbot
VITE_N8N_WEBHOOK_URL=

# Modelo por defecto en Ollama (opcional, también puede fijarse en n8n)
# Ejemplos: llama3.2, llama3.1, mistral, qwen2.5
VITE_OLLAMA_MODEL=llama3.2

# Habilita el widget flotante del chatbot en la UI
VITE_CHATBOT_ENABLED=true


# =====================================================
# Netlify Deploy (GitHub Actions)
# =====================================================
# Estas variables NO van en .env de producción, se agregan como
# Secrets en GitHub: Settings → Secrets and variables → Actions
# NETLIFY_AUTH_TOKEN: Token de acceso personal desde Netlify
# NETLIFY_SITE_ID: ID del sitio desde la configuración del sitio en Netlify
# Se listan aquí para referencia únicamente. NUNCA commitees valores reales.
NETLIFY_AUTH_TOKEN=
NETLIFY_SITE_ID=